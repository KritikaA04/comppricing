const typeArray =
        (isIntentVisible && is4to24case && !isVoidable) ? ["Cancel type", "PNR status", "Payment Line", "Cancel Fee", "Priority Refund"] :
            (isIntentVisible && is4to24case) ? ["Cancel type", "PNR status", "Payment Line", "Cancel Fee"] :
                (isIntentVisible && !isVoidable) ? ["Cancel type", "PNR status", "Payment Line", "Priority Refund"] :
                    (isIntentVisible) ? ["Cancel type", "PNR status", "Payment Line"] :
                        ["Cancel type", "PNR status"];
   isBookingCancelledFromOAP=true;


return (
        <div className="report-error-popup report-error-popup-show">
            <div className="error-header pb-1" style={{ fontSize: "13px" }}>Report Error</div>
            {typeArray.map((e, index) => {
                return (
                    <div>
                        <input type="checkbox" checked={reportErrorObject[e]} onClick={(event) => handleToggle(event.target.value)} id={index} value={e} data-testid={e} />
                        <label htmlFor={index} className="px-2" style={{ fontSize: "13px" }}>{e}</label>
                    </div>
                )
            })}
